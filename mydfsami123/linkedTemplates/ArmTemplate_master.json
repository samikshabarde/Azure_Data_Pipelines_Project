{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mydfsami123"
		},
		"AzureSQLDatabase_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSQLDatabase'"
		},
		"NormalDataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'NormalDataLake'"
		},
		"SynapseDataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'SynapseDataLake'"
		},
		"AzureSQLDatabase_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "samiserver123.database.windows.net"
		},
		"AzureSQLDatabase_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db_nycpayroll"
		},
		"AzureSQLDatabase_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminlogin123"
		},
		"NormalDataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://samistorage123.dfs.core.windows.net/"
		},
		"SynapseDataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://synapsedatalake123.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mydfsami123_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSQLDatabase_password": {
						"value": "[parameters('AzureSQLDatabase_password')]"
					},
					"NormalDataLake_accountKey": {
						"value": "[parameters('NormalDataLake_accountKey')]"
					},
					"SynapseDataLake_accountKey": {
						"value": "[parameters('SynapseDataLake_accountKey')]"
					},
					"AzureSQLDatabase_properties_typeProperties_server": {
						"value": "[parameters('AzureSQLDatabase_properties_typeProperties_server')]"
					},
					"AzureSQLDatabase_properties_typeProperties_database": {
						"value": "[parameters('AzureSQLDatabase_properties_typeProperties_database')]"
					},
					"AzureSQLDatabase_properties_typeProperties_userName": {
						"value": "[parameters('AzureSQLDatabase_properties_typeProperties_userName')]"
					},
					"NormalDataLake_properties_typeProperties_url": {
						"value": "[parameters('NormalDataLake_properties_typeProperties_url')]"
					},
					"SynapseDataLake_properties_typeProperties_url": {
						"value": "[parameters('SynapseDataLake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "mydfsami123_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'mydfsami123_ArmTemplate_0')]"
			]
		}
	]
}